{% extends 'base.html.twig' %}

{% block title %}Song-Overview{% endblock %}

{% block body %}
    <div class="table-responsive">
        <table class="table">
            {% for song in songs %}
                <tr>
                    <td>
                        <twig:Song song="{{ song }}" />
                    </td>
                </tr>
            {% endfor %}
        </table>
    </div>
{% endblock %}